      <dependency id="PocketLogger" version="[0.4.0,0.5)" include="all" />
      <dependency id="Pocket.Disposable" version="[1.0.4,2)" include="all" />
      <dependency id="Pocket.TypeDiscovery" version="[0.5.0,)" include="all" />
    </dependencies>
  </metadata>
